<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="Autofill,Hardcoded">

    <TextView
        android:id="@+id/nameNpos"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="left"
        android:text="Enter Name And Position"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/name"
        android:layout_width="411dp"
        android:layout_height="45dp"
        android:hint="Enter Name"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="30dp" />

    <EditText
        android:id="@+id/pos"
        android:layout_width="411dp"
        android:layout_height="45dp"
        android:hint="Enter Position"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintTop_toBottomOf="@+id/name"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="413dp"
        android:layout_height="39dp"
        android:text="Enter Educatiom"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="125dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="Enter University Name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.004"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="170dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="412dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Enter degree"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="218dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="408dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="number"
        android:hint="Enter Year"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="266dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="409dp"
        android:layout_height="45dp"
        android:text="Enter Course"
        android:textStyle="bold"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"

        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="314dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="408dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Enter Organization name"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="363dp" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="408dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Enter Course Name"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="407dp" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="410dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:hint="Enter Year"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="457dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="402dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="text"
        android:hint="Enter GitHub UserName"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="513dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SUBMIT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="639dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
